    "language": "en",
    "output_format": "mp3_44100_128",
    "emotion_note": "pleading_fearful",
    "tags": ["anxious", "desperate", "self-preservation"]
  },
  {
    "id": "scene1_zack_11",
    "character": "Zack",
    "text": "Webb's dead. The rules changed.",
    "voice_name": "Zack",
